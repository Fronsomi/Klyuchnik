package com.example.klyuchnik;

public class PersonModal {

    // переменные для ФИО,
    // номера карты, времени и номера аудитории, id.
    private String FullName;
    private String CardNumber;
    private String Time;
    private String AudienceNumber;
    private int id;

    // создание методов получения и установки
    public String getFullName() { return FullName; }

    public void setFullName(String FullName)
    {
        this.FullName = FullName;
    }

    public String getCardNumber()
    {
        return CardNumber;
    }

    public void setCardNumber(String CardNumber)
    {
        this.CardNumber = CardNumber;
    }

    public String getTime() { return Time; }

    public void setTime(String Time)
    {
        this.Time = Time;
    }

    public String getAudienceNumber()
    {
        return AudienceNumber;
    }

    public void setAudienceNumber(String AudienceNumber)
    {
        this.AudienceNumber = AudienceNumber;
    }

    public int getId() { return id; }

    public void setId(int id) { this.id = id; }

    // конструктор
    public PersonModal(String FullName,
                       String CardNumber,
                       String Time,
                       String AudienceNumber)
    {
        this.FullName = FullName;
        this.CardNumber = CardNumber;
        this.Time = Time;
        this.AudienceNumber = AudienceNumber;
    }
}
